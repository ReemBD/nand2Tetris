/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */

Chip MUX {
    IN a, b, sel;
    OUT out;

    PARTS:
      And(a=sel,b=b, out=w1);
      Not(in=s, out=notsel);
      And(a=notsel,b=a, out=w2);
      Or(a=w1,b=w2,out=out);
}